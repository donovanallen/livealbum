<div class="artistpage" id="albumindex">

<h2>Your Showlists</h2>
<h5><%#= link_to "Add a new album", new_artist_album_path(@artist), class: "link" %> </h5>
<br>
<% @showlists.each do |showlist| %>
<p>
  <%= link_to showlist.name, showlist_path(showlist), class: "link", id: "showlistlisting" %>
</p>
<% end %>
</div>
