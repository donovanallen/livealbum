<div class="artistpage" id="albumindex">

<h2>Albums</h2>
<h5><%#= link_to "Add a new album", new_artist_album_path, class: "link" %> </h5>

<% @albums.each do |album| %>
<p>
  <%= link_to album.title, album_path(album.id), class: "link", id: "artistlisting" %>
  <h4> <%= album.artist.name %> </h4>
</p>
<br>
<% end %>
</div>
