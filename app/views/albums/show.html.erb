<img src="<%= @album.photo_url %>" />
<h3><%= @album.title %> </h3>
<h4><%= @album.artist.name %> </h4>

<% @album.shows.each do |show| %>
<p>
<%= link_to show.title, show_path(show), class: "link" %>
</p>
<% end %>

<footer>
<%= link_to "Back to Albums", artist_path(@album.artist), class: "link" %>
</footer>
